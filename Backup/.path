# give Homebrew priority
PATH="/usr/local/bin:/usr/local/sbin:/usr/local/Cellar:$PATH"

# use coreutils without prefix of g (e.g. ls instead of gls)
PATH="$(brew --prefix)/opt/coreutils/libexec/gnubin:$PATH"

# node.js to the back of the line
PATH="$PATH:/usr/local/share/npm/bin"

# our local shenanigans
PATH="$PATH:$HOME/bin"

# for sebglazebrook/aliases
[ -e "${HOME}/.aliases.d/shims" ] && PATH="${HOME}/.aliases.d/shims:${PATH}"


export PATH
